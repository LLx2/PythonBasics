    "import string\n",
    "\n",
    "def panagramChecker(s):\n",
    "    alphabet = string.lowercase \n",
    "    a = list(alphabet)\n",
    "    count = 0\n",
    "    s = s.lower()\n",
    "\n",
    "    for letter in s:\n",
    "        if letter in a:\n",
    "            count = count + 1\n",
    "    if count > len(a):\n",
    "        print(\"is panagram: \"), count\n",
    "    else:\n",
    "        print(\"is not a panagram: \"), count\n",
    "\n",
