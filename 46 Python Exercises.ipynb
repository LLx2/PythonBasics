   "cell_type": "code",
   "execution_count": 18,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "def BottlesOfBeer():\n",
    "    BottleCount = 99\n",
    "    for i in range(100):\n",
    "            print \"%d Bottles of beer on the wall %d bottles of beer. Take one down, pass it around\" % (BottleCount, BottleCount)\n",
    "            BottleCount = BottleCount - 1\n",
    "            if BottleCount < 1:\n",
    "                print \"No more Beer, bitches\"\n",
    "                break\n",
    "            \n",
    "\n",
    "# BottlesOfBeer()"
   ]
